{
  "name": "md-blog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
    "dev:proxy": "prisma-accelerate-local postgresql://postgres:password@localhost:15432/postgres -p 8000",
    "dev:scss": "typed-scss-modules src -i ./src/styles --ignore **/global.scss -w -L error",
    "dev:codegen": "graphql-codegen -c codegen/codegen.json -w",
    "dev:next": "next",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "start:standalone": "cd .next/standalone && node server.js",
    "prisma": "npm-run-all -p prisma:*",
    "prisma:migrate": "prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate --no-engine",
    "sf": "scaffold create -t Templates/Component",
    "test": "yarn storybook:build && npm-run-all -p -r storybook:start storybook:test",
    "storybook": "storybook dev -p 9001",
    "storybook:build": "storybook build",
    "storybook:start": "http-server -s -p 9001 storybook-static",
    "storybook:test": "test-storybook --url http://127.0.0.1:9001 --coverage",
    "lint:fix": "next lint --fix src && prettier -w src",
    "ts-test": "next-exec -c production -- ts-node -r tsconfig-paths/register -T -O {\\\"module\\\":\\\"commonjs\\\"} src/tools/test",
    "docker:start": "docker compose -p next-blog -f docker/production/docker-compose.yml up ",
    "docker:build": "docker compose -p next-blog -f docker/production/docker-compose.yml build --parallel --compress --pull",
    "docker:upload": "docker save  next-blog-next-blog | env-exec -e .env-deploy docker load",
    "docker:log": "env-exec -e .env-deploy -- docker compose -p next-blog -f docker/production/docker-compose.yml logs",
    "docker:deploy": "yarn docker:build && yarn docker:upload && env-exec -e .env-deploy -- docker compose -p next-blog -f docker/production/docker-compose.yml up -d"
  },
  "dependencies": {
    "@apollo/explorer": "^3.6.0",
    "@firebase/app": "^0.9.25",
    "@firebase/auth": "^1.5.1",
    "@monaco-editor/react": "^4.6.0",
    "@node-libraries/wasm-webp-encoder": "^1.0.5",
    "@pothos/core": "^3.40.0",
    "@pothos/plugin-prisma": "^3.62.0",
    "@pothos/plugin-prisma-utils": "^0.14.0",
    "@prisma/client": "^5.7.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "@react-libraries/next-exchange-ssr": "^1.0.3",
    "daisyui": "^4.4.19",
    "firebase-storage": "^1.0.0",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.2",
    "jose": "^5.1.3",
    "monaco-editor": "^0.45.0",
    "next": "^14.0.4",
    "pothos-prisma-generator": "^1.0.2",
    "pothos-schema-exporter": "^1.0.0",
    "pure-uuid": "^1.8.1",
    "react": "^18.2.0",
    "react-daisyui": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.0",
    "react-icons": "^4.12.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.0",
    "urql": "^4.0.6",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@node-libraries/scaffold": "^1.0.3",
    "@node-libraries/semaphore": "^1.0.3",
    "@parcel/watcher": "^2.3.0",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "^7.6.4",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "autoprefixer": "^10.4.16",
    "env-exec": "^1.0.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "next-exec": "^0.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prisma": "^5.7.0",
    "prisma-accelerate-local": "^0.1.4",
    "sass": "^1.69.5",
    "storybook": "^7.6.4",
    "storybook-addon-module-mock": "^1.1.2",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
