{
  "name": "md-blog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
    "dev:scss": "typed-scss-modules src -i ./src/styles --ignore **/global.scss -w -L error",
    "dev:next": "next",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:standalone": "cd .next/standalone && node server.js",
    "prisma:migrate": "prisma format && next-exec prisma migrate dev",
    "prisma:generate": "prisma generate",
    "codegen:generate": "graphql-codegen -d -c codegen/codegen.json",
    "generate:all": "npm-run-all prisma:migrate prisma:generate codegen:generate",
    "sfc": "scaffold create -t Templates/Component",
    "sfs": "scaffold create -t Templates/Schema -o src/server/schema",
    "test": "yarn storybook:build && npm-run-all -p -r storybook:start storybook:test",
    "storybook": "storybook dev -p 9001",
    "storybook:build": "storybook build",
    "storybook:start": "http-server -s -p 9001 storybook-static",
    "storybook:test": "test-storybook --url http://127.0.0.1:9001 --coverage",
    "lint:fix": "next lint --fix src && prettier -w src",
    "restoreFile": "next-exec ts-node -r tsconfig-paths/register -T -O {\\\"module\\\":\\\"commonjs\\\"} src/tools/importFile",
    "backupFile": "next-exec -c production -- ts-node -r tsconfig-paths/register -T -O {\\\"module\\\":\\\"commonjs\\\"} src/tools/exportFile",
    "ts-test": "next-exec -c production -- ts-node -r tsconfig-paths/register -T -O {\\\"module\\\":\\\"commonjs\\\"} src/tools/test",
    "docker:start": "docker compose -p next-blog -f docker/production/docker-compose.yml up ",
    "docker:build": "docker compose -p next-blog -f docker/production/docker-compose.yml build --parallel --compress --pull",
    "docker:upload": "docker save  next-blog-next-blog | env-exec -e .env-deploy docker load",
    "docker:log": "env-exec -e .env-deploy -- docker compose -p next-blog -f docker/production/docker-compose.yml logs",
    "docker:deploy": "yarn docker:build && yarn docker:upload && env-exec -e .env-deploy -- docker compose -p next-blog -f docker/production/docker-compose.yml up -d"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@firebase/app": "^0.9.18",
    "@firebase/auth": "^1.3.0",
    "@monaco-editor/react": "^4.5.2",
    "@mui/core": "^5.0.0-alpha.54",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.9",
    "@mui/styled-engine": "^5.14.9",
    "@mui/styles": "^5.14.9",
    "@mui/system": "^5.14.9",
    "@mui/x-date-pickers": "^6.13.0",
    "@node-libraries/dynamic-import": "^1.0.6",
    "@node-libraries/wasm-webp-encoder": "^1.0.5",
    "@paljs/plugins": "^6.0.7",
    "@pothos/core": "^3.35.0",
    "@pothos/plugin-prisma": "^3.59.2",
    "@pothos/plugin-prisma-utils": "^0.11.0",
    "@prisma/client": "^5.3.0",
    "@react-libraries/next-apollo-server": "^1.0.0",
    "@react-libraries/next-exchange-ssr": "^1.0.0",
    "cookie": "^0.5.0",
    "date-fns": "^2.30.0",
    "firebase-admin": "^11.10.1",
    "graphql": "^16.8.0",
    "jsonwebtoken": "^9.0.2",
    "monaco-editor": "^0.43.0",
    "next": "^13.4.19",
    "pothos-prisma-generator": "^0.0.12",
    "pothos-schema-exporter": "^0.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "urql": "^4.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@node-libraries/scaffold": "^1.0.2",
    "@node-libraries/semaphore": "^1.0.2",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/jest": "^0.2.2",
    "@storybook/nextjs": "^7.4.1",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/cookie": "^0.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "^18.2.21",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "autoprefixer": "^10.4.15",
    "env-exec": "^1.0.1",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "next-exec": "^0.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prisma": "^5.3.0",
    "sass": "^1.67.0",
    "storybook": "^7.4.1",
    "storybook-addon-module-mock": "1.0.18",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typed-scss-modules": "^7.1.4",
    "typescript": "^5.2.2"
  }
}
